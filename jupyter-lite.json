{
  "jupyter-lite-schema-version": 0,
  "jupyter-lite-root": ".",
  "apps": [
    "lab",
    "repl",
    "edit",
    "tree",
    "notebooks"
  ],
  "disabled_extensions": [
    "fspath"
  ],
  "jupyter-config-data": {
    "contentsStorageName": "JupyterLite Storage - Local",
    "fileTypes": {
      "json": {
        "name": "json",
        "extensions": [".json"],
        "mimeTypes": ["application/json"],
        "fileFormat": "text"
      }
    }
  },
  "settings_overrides": {
    "@jupyterlite/application-extension:shell": {
      "devMode": true,
      "serviceWorker": {
        "scope": "/static/jupyterlite/",
        "enabled": false
      }
    },
    "@jupyterlite/pyodide-kernel-extension:kernel": {
      "mountDrive": true,
      "disablePyPIFallback": false
    },
    "@jupyterlab/filebrowser-extension:browser": {
      "showHiddenFiles": false
    }
  }
} 